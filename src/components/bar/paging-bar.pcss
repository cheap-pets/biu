.mu-paging-bar {
  margin-top: $halfSpacingSize;

  & > label {
    display: inline-block;
    min-width: 80px;
    text-align: center;
  }

  & [contenteditable] {
    line-height: 16px;
    border: 1px solid $normalBorderColor;
    border-radius: 3px;
    padding: 3px 8px;
    margin: 0 8px;
    outline: none;
  }

  & .mu-button {
    height: 24px;
    padding: 0 10px;
  }

  & .mu-icon-button {
    width: 24px;
    padding: 0;
  }
}
