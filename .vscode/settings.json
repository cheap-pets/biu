{
  "path-autocomplete.pathMappings": {
    "@/": "${folder}/src",
    "~icons/": "${folder}/node_modules/@tabler/icons/icons"
  },
  "cSpell.ignorePaths": [
    "dist",
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git",
    ".vscode",
    ".vscode-insiders"
  ],
  "cSpell.words": [
    "colorette",
    "itemclick",
    "stylelint",
    "tabler"
  ],
  "cSpell.ignoreWords": [
    "panend",
    "panmove",
    "panstart",
    "panx",
    "panxend",
    "panxmove",
    "panxstart",
    "pany",
    "panyend",
    "panymove",
    "panystart",
    "sizechange",
    "timespan"
  ]
}